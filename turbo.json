{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["build"],
      "cache": false,
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
